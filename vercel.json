{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/templates/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "frontend/static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.py"
    },
    {
      "src": "/health",
      "dest": "/api/index.py"
    },
    {
      "src": "/test-db",
      "dest": "/api/index.py"
    },
    {
      "src": "/test-env",
      "dest": "/api/index.py"
    },
    {
      "src": "/debug-analytics",
      "dest": "/api/index.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/frontend/static/$1"
    },
    {
      "src": "/",
      "dest": "/frontend/templates/index.html"
    }
  ]
}
